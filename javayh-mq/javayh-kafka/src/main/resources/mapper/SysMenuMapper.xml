<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.javayh.mapper.SysMeunMapper">

    <update id="modifyById"  parameterType="com.javayh.entity.SysMenu">
        update
          sys_menu
        set
           pcode = #{pcode}
        WHERE
            id = #{id}
    </update>

</mapper>
